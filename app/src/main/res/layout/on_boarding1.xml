<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutAnimation="@transition/anim_setting_right"
    android:background="@color/orange"

    android:gravity="center_horizontal|center_vertical"
    android:orientation="vertical"
    >


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="437dp"
        android:layout_height="524dp"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/il2"
        tools:layout_editor_absoluteX="-13dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fastest Delivery \n          24/7"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageButton
        android:id="@+id/isecondpage_withoutinter_but"
        android:layout_width="17dp"
        android:layout_height="21dp"


        android:layout_marginEnd="8dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/firstpage"
        app:srcCompat="@drawable/oval_white"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/firstpage"
        android:layout_width="17dp"
        android:layout_height="21dp"
        android:layout_marginEnd="190dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/oval" />
</androidx.constraintlayout.widget.ConstraintLayout>